#include<stdio.h>
#include<windows.h>
#include<time.h>

int main(void)
{
	int endTime =(unsigned)time(NULL);
	endTime += 6;

	while (1) {


		int startTime = (unsigned)time(NULL);
		printf("%d초\n", endTime - startTime);
		if (endTime - startTime == 0)
		{
			printf("종료되었습니다.");
			return 0;
		}

	}








}
