#include<stdio.h>
#include<Windows.h>

void gotoxy(int x, int y)
{
	COORD CursorPosition = { x, y };
	SetConsoleCursorPosition(GetStdHandle(STD_OUTPUT_HANDLE), CursorPosition);
}




int main(void)
{
	gotoxy(0, 3);
	printf("(0,3)에 좌표에 출력했습니다.");
	gotoxy(0, 4);
	printf("(0,4) 좌표에 출력했습니다");
	gotoxy(0, 5);
	printf("(0,5) 좌표에 출력했습니다 / ");







}
